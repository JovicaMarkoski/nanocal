{
  "private": true,
  "scripts": {
    "predev": "tsc",
    "dev": "tsc -w & npm run dev:nanocal & npm run dev:ranger & serve -p 3000",
    "dev:nanocal": "rollup nanocal/src/main.js -c rollup.config.js -w -f es -o dist/nanocal.js",
    "dev:ranger": "rollup ranger/src/main.js -c rollup.config.js -w -f es -o dist/ranger.js",
    "format": "prettier-standard '**/*.ts'",
    "lint": "tslint --project tsconfig.json 'src/**/*.ts'",
    "pretest:dev": "tsc -p test/tsconfig.json",
    "test:dev": "tsc -p test/tsconfig.json -w & watchify test/entry.js -o test/test_bundle.js & serve -p 3000 test",
    "pretest": "tsc -p test/tsconfig.json && browserify test/entry.js -o test/test_bundle.js",
    "test": "node test/index.js"
  },
  "dependencies": {
    "cntdys": "^0.2.8"
  },
  "devDependencies": {
    "@types/tape": "^4.2.32",
    "bundlesize": "^0.17.0",
    "cntdys": "^0.2.8",
    "ecstatic": "^3.2.0",
    "prettier-standard": "^8.0.1",
    "puppeteer": "^1.4.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel-minify": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-svelte": "^4.1.0",
    "serve": "^6.5.6",
    "svelte": "^2.5.1",
    "tape": "^4.9.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3",
    "watchify": "^3.11.0"
  }
}
