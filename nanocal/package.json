{
  "name": "nanocal",
  "version": "0.2.0",
  "main": "dist/nanocal.cjs.js",
  "module": "dist/nanocal.esm.js",
  "browser": "dist/nanocal.min.iife.js",
  "repository": "https://github.com/zigomir/nanocal/tree/master/nanocal",
  "license": "MIT",
  "homepage": "https://nanocal.zigomir.com",
  "bugs": "https://github.com/zigomir/nanocal/issues",
  "description": "Minimal, AirBnb inspired, date picker",
  "dependencies": {
    "cntdys": "^0.2.5"
  },
  "scripts": {
    "build": "../node_modules/.bin/tsc && npm run build:cjs && npm run build:module && npm run build:iife && ../node_modules/.bin/bundlesize",
    "build:cjs": "../node_modules/.bin/rollup src/main.js -c ../rollup.config.js -f cjs -o dist/nanocal.cjs.js",
    "build:module": "../node_modules/.bin/rollup src/main.js -c ../rollup.config.js -f es -o dist/nanocal.esm.js",
    "build:iife": "../node_modules/.bin/rollup src/main.js -c ../rollup.config.js -f iife -o dist/nanocal.min.iife.js",
    "test": "npm run --prefix .. test",
    "prepublishOnly": "npm run build && npm run test"
  },
  "bundlesize": [
    {
      "path": "./dist/nanocal.min.iife.js",
      "maxSize": "4.5 kB"
    }
  ]
}
